
<div class="container margin-top50">
  <%= form_for @student  do |f| %>
    <div class=" margin-top10 form">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="margin-top10 form">
      <%= f.label :last_name %>
      <%= f.text_field :last_name %>
    </div>
    <div class="margin-top10 form">
      <%= f.label :address %>
      <%= f.text_field :address ,id:"search" %>
    </div>
    <div class="margin-top10 form">
      <%= f.label :code %>
      <%= f.text_field :code %>
    </div>



      <%= f.submit "Enviar" ,class:"button buttonForm" %>

  <% end %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyAxU9Z0c2gm5oEg64_FYukuJLhMHoKe_TY">
  </script>
</div>
