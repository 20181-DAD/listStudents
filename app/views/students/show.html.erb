
<p>
  <strong>Name:</strong>
  <%= @student.name %>
</p>
<p>
  <strong>Apellido:</strong>
  <%= @student.last_name %>
</p>
<p>
  <strong>Direccion:</strong>
  <%= @student.address %>
</p>
<p>
  <strong>Codigo:</strong>
  <%= @student.code %>
</p>

<% if @student.latitude.nil? || @student.longitude.nil? %>
  <p>No pudimos geolocalizarte, intenta editar tu direcci√≥n</p>
<% else %>
  <div id="map-geolocation" class="map-geolocation" data-lat="<%= @student.latitude %>" data-lng="<%= @student.longitude %>">
  </div>
<% end %>
<p><%= link_to 'regresar', students_path %></p>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAxU9Z0c2gm5oEg64_FYukuJLhMHoKe_TY">
</script>
